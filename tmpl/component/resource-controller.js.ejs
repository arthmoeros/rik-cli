<% if (resource.type === 'multi-implementation') { -%>
// This is a default Controller Implementation for a Multi Controller pattern
class DefaultController {
<% } else { -%>
// This is a resource Controller, all API calls for the resource will be handled here
class MyController {
<% } -%>

  <% resource.verbs.forEach((verb) => { %>async <%=verb%>(params, query, header, body) {
    return {
      header: {
        // Your response headers here
      },
      body: {
        // Your response body here
      }
    };
  }

  <% }); %>
}
<% if (resource.type === 'multi-implementation') { -%>
module.exports = DefaultController;
<% } else { -%>
module.exports = MyController;
<% } -%>